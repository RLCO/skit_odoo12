<odoo>
	<template id="contact_us">
		<t t-call="website.layout">
			<t t-set="title">Contact</t>
			<div class="oe_structure">

				<section class="sks_sectionProducts " style="height: 60em;">
					<div class="container">
						<div class="row">
							<div class="sks-slide slide-five" data-background="#ffffff">
								<div class="inside title_fix_cntr">
									<h1>
										<b>
											<font style="color: rgba(230, 230, 230);" class="slide5-title">Contact</font>
										</b>
									</h1>
								</div>
							</div>
						</div>
					</div>
				</section>
				
				<section style="height: 60em;">
					<div class="container sks_container">
						<div class="row">
							<div class="sks-slide slide-three sks-sec2-contact-slide-div"
								data-background="#f7af00">
								<div class="inside sks-sec2-contact-inside1">
									<div class="sks-sec1-contact-div" />
								</div>
								<div class="inside sks-sec2-contact-inside2">
									<div class="sks-sec2-contact-inside-div1">
										<div class="sks-sec2-contact-inside-div2">
											<h2>Contact Info</h2>
											<p style="text-align: left;">No 2, 4th Cross Street</p>
											<p style="text-align: left;">Sidhanantha Nagar</p>
											<p style="text-align: left;">Near IG Square</p>
											<p style="text-align: left;">Puducherry</p>
										</div>
										<div class="sks-sec2-contact-inside-div2" style=" float: right; color: #fff;">
											<h2 style="visibility: hidden;">Contact Info</h2>
											<p style="text-align: left;">info@srikeshinfotech.com</p>
											<p style="text-align: left;">0123456789</p>
										</div>
									</div>
									<div class="sks_float_form_content" id="message">
									<form action="#" class="sks_float_form sks_contact_form">
									<div class="phone-container">
										<span style="color: #fff;">Hey, my name is</span>
										<input style="outline: none; width: 15.6em;"
											class="phone-input sks_contact_name" t-attf-ismandatorty="true" type="text" name="name"
											placeholder="full name" />
										,


									</div>
									<div class="phone-container">
										<span style="color: #fff;">and I work at</span>
										<input style="outline: none; width: 17.3em;"
											class="phone-input sks_contact_company" t-attf-ismandatorty="true" type="text" name="name"
											placeholder="company" />
										,


									</div>
									<div class="phone-container">
										<span style="color: #fff;">You can reach me at</span>
										<input style="outline: none; width: 13.7em;"
											class="phone-input sks_contact_email" t-attf-ismandatorty="true" type="text" name="name"
											placeholder="email" />
										,


									</div>
									<div class="phone-container">
										<span style="color: #fff; visibility: hidden;">You can reach me at</span>
										<input style="outline: none; width: 13.7em;"
											class="phone-input sks_contact_phone" t-attf-ismandatorty="true" type="text" name="name"
											placeholder="phone no" />
										,


									</div>
									<div class="phone-container">
										<span style="color: #fff;">If you have any queries</span>
										<input style="outline: none; width: 12.4em;"
											class="phone-input sks_contact_project" t-attf-ismandatorty="true" type="text" name="name"
											placeholder="message" />
										,


									</div>
									<br></br><br></br>
									<button type="submit" id="sks-contact-launch" class="sks_float_btn sks-contact-launch"><i class="fa fa-paper-plane"></i><![CDATA[&nbsp;]]><![CDATA[&nbsp;]]>Launch
									</button>
									</form>
									</div>
				 					<div id="confirm">
				 						<div id="popup">
				 							<div class="popup_div">
				 								<canvas style="display: none" width="100" height="100"></canvas>
				 								<img id="confirm_gif_id" class="confirm_gif" alt="Contact us." src="/skit_snippets/static/src/img/float_video/confirm.gif"/>
				 								<span class="pop_span">Thank for registering we will contact you soon
				 								</span>
				 							</div>
				 						</div>
				   					</div>
								</div>
							</div>
						</div>
					</div>
				</section>
				<style>
					* {
					box-sizing: border-box
					}

					body {
					font-family: 'proxima-nova', sans-serif;
					font-family: inherit;
					transition: background 1s ease;
					background: #ffffff;
					}

					p {
					font-family: 'proxima-nova', sans-serif;
					color: #ecf0f1;
					font-size: auto;
					text-align: center;
					}

					span {
					font-family: 'proxima-nova', sans-serif;
					clear: both;
					font-size: auto;
					color: inherit;
					}

					a {
					font-family: 'proxima-nova', sans-serif;
					color: #c0392b;
					text-decoration: none;
					}

					.sks-slide { .inside { display:table;
					height: 100%;
					width: 100%;
					padding: 0 3em; p { display : table-cell;
					width: 100%;
					clear: both;
					vertical-align: middle;
					text-align: center;
					font-family: 'proxima-nova', sans-serif;
					}

					}
					}
					.navbar {
					width: 100%;
					background-color: transparent !important;
					}
				</style>
			</div>
		</t>
	</template>
</odoo>