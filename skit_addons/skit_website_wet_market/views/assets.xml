            <link rel="stylesheet" type="text/scss" href="/skit_website_wet_market/static/src/css/sale_order.css"/>
        </xpath> 
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/skit_website_wet_market/static/src/js/sale_order.js"></script>
             <script type="text/javascript" src="/skit_website_wet_market/static/src/js/product_configuration.js"></script>
        </xpath>
    </template>
    
    <template id="skit_website_frontend" inherit_id="website.assets_frontend" name="Skit Website Assets">
