<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

	<t t-name="HotelManagementWidget">
        <div class='product-list-container'>
            <div class="product-list-scroller touch-scrollable">
                <div class="hotel">
                	<span>Hotel Management</span>
                </div>
            </div>
            <span class="placeholder-ScrollbarWidget" />
        </div>
    </t>

	<t t-name="HotelManagementScreenWidget">
       <div class="hotel-screen screen">
       		<span>dfhjdskfhjfdk</span>
       		<input type="date" name="my_date" />
        	<span class="vendor-dash-button">Vendor Dashboard</span>
        </div>
    </t>
    
    <t t-name="VendorListContent">
    	<div style="display: inline-block;width: 100%;height: 100%;overflow-y: auto;">
    		<h1 t-attf-style="text-align: left;border-bottom: 0.1px solid gray;color:#{color}"><t t-esc="form_name" /></h1>
    		<!-- Kanban View -->
    		<t t-if="form_view == 'kanban'">
    			<t t-set="vcatg_id" t-value="0" />
		    	<t t-foreach="result_datas" t-as="vendor" >
		    		<t t-if="(is_other) and (vcatg_id != vendor.category_id)">
			    		<div style="display: inline-block;width: 100%;">
			    			<span t-attf-style="float: left;color:#{color}"><t t-esc="vendor.category_name" /></span>
			    		</div>
		    		</t>
					<div class="vendor-tag" >
						<div class="vendor-list">
							<t t-foreach="line_group_key" t-as="line_key" >
								<t t-set="first_col" t-value="line_group[line_key][0]" />
							 	<t t-if="(first_col.form_field_type)=='image'">
									<div class="vendor-image">
										<img class="header-text" t-att-src='widget.vendor_image_url(vendor.id)' alt="Vendor Iamge"/>
									</div>
								</t>
							</t>
							<div class="vendor-addr">
								<t t-set="count" t-value="1" />
								<t t-foreach="line_group_key" t-as="line_key" >
									<t t-if="count != 1" >
										<t t-foreach="line_group[line_key]" t-as="same_row">
											<t t-if="(same_row.form_field_type)=='label'">
												<ul>
										        	<li t-if="vendor[same_row.form_fields]" t-attf-style="font-size:#{same_row.font_size}px; font-style:#{same_row.font_style}; font-family:#{same_row.font_family};"><t t-esc="vendor[same_row.form_fields]" /></li>
												</ul>
											</t>
										</t> 
									</t>
									<t t-set="count" t-value="count + 1" />
								</t>
							</div>
						</div>
						<div class="vendor-process">
							<ul>
								<!-- <li class="order_icon" style="background-color: #00BFFF;"><img  src="/skit_pos_hotel_management/static/src/img/order_icon.png" width="24" height="21" /></li>
								<li class="delivery_icon" style="background-color: #483D8B;"><img  src="/skit_pos_hotel_management/static/src/img/delivery_icon.png" width="24" height="21" /></li>
								<li class="payment_icon" style="background-color: #819FF7;"><img  src="/skit_pos_hotel_management/static/src/img/payment_icon.png" width="24" height="21" /></li> -->
								<t t-foreach="line_group_key" t-as="line_key" >
									<t t-foreach="line_group[line_key]" t-as="same_row">
										<t t-if="(same_row.form_field_type)=='sub_form'">
											<t t-foreach="sub_form_template[same_row.sub_template_id]" t-as="temp" >
												<li t-attf-id="#{temp.id}" t-attf-vendor="#{vendor.id}" class="vendor-form-icon" t-attf-style="background-color: #{temp.color}">
													<img  t-att-src='widget.sub_template_line_icon_url(temp.id)' width="24" height="21" />
												</li>
											</t>
										</t>
									</t>
								</t>
							</ul>
						</div>
					</div>
				<t t-set="vcatg_id" t-value="vendor.category_id" />
		    	</t>
	    	</t>
	    	
	    	<!-- List View -->
	    	<t t-if="form_view == 'list'">
	    		<table id="vendor_order_list" class="table stripe hover row-border order-column dataTable no-footer">
	    			<thead>
	    				<t t-set="head_count" t-value="1" />
	    				<t t-foreach="line_group_key" t-as="line_key" >
	    					<t t-if="head_count == 1" >
			    				<tr style="background-color: #875A7B;color: white;">
			    					<!-- <t t-foreach="sub_form_template[line_group[line_key].]" t-as="temp" >
			    						<th><t t-esc="temp.name" /></th>
			    					</t> -->
						    		<t t-foreach="line_group[line_key]" t-as="label">
						    			<t t-if="(label.form_field_type)=='sub_form'">
						    				<t t-foreach="sub_form_template[label.sub_template_id]" t-as="temp" >
						    					<th><t t-esc="temp.name" /></th>
						    				</t>
						    			</t>
						    			<t t-else="" >
						    				<th><t t-esc="label.form_label" /></th>
						    			</t>
						    		</t>
					    		</tr>
					    	<t t-set="head_count" t-value="head_count + 1" />
				    		</t>
			    		</t>
	    			</thead>
	    			<tbody>
	    				<t t-set="count" t-value="1" />
	    				<t t-foreach="result_datas" t-as="order" >
		    				<t t-foreach="line_group_key" t-as="line_key" >
		    					<t t-set="first_col" t-value="line_group[line_key][0]" />
		    					<!-- <t t-if="count == 1" >
			    					<tr style="background-color: #875A7B;color: white;">
			    						<t t-foreach="line_group[line_key]" t-as="label">
			    							<th><t t-esc="label.form_label" /></th>
			    						</t>
			    					</tr>
		    					</t> -->
		    					<tr>
		    						<t t-if="(first_col.form_field_type)=='sub_form'">
			    						<t t-foreach="sub_form_template[first_col.sub_template_id]" t-as="temp" >
				    						<td>
				    							<span t-attf-id="#{temp.id}" t-attf-orderid="#{order.id}" class="edit-datatable" ><img t-att-src='widget.sub_template_line_icon_url(temp.id)' width="20" height="17" /></span>
				    						</td>
			    						</t>
			    					</t>
			    					<t t-else="" >
			    						<td>
			    							<t t-if="(first_col.form_field_type)=='label'">
			    								<span><t t-esc="order[first_col.form_fields]" /></span>
			    							</t>
			    						</td>
		    						</t>
		    						<t t-foreach="line_group[line_key]" t-as="same_row">
		    							<t t-if="same_row.sameline">
		    								<td>
				    							<t t-if="(same_row.form_field_type)=='label'">
				    								<span><t t-esc="order[same_row.form_fields]" /></span>
				    							</t>
				    						</td>
		    							</t>
		    						</t>
		    					</tr>
		    				</t>
		    			<t t-set="count" t-value="count + 1" />
	    				</t>
	    			</tbody>
	    		</table>
	    	</t>
	    	<!-- Form View -->
	    	<t t-if="form_view == 'form'">
	    		<div style="display: inline-block;width: 100%;">
	    			<button class="back-btn" style="float: left;background: #147FD0;color: white;">Back</button>
	    		</div>
	    		<div>
	    		<t t-if="line_group_key">
	    			<t t-foreach="current_order" t-as="c_order" >
	    			<div style="width:98%;border: 1px solid gray;">
		    			<table class="order-form-detail">
		    				<tbody>
		    				<div style="width:100%;border-bottom: 1px solid gray;background-color: #F8F8F8;display: inline-block;">
		    					<div>
			    					<t t-foreach="line_group_key" t-as="line_key" >
			    						<t t-foreach="line_group[line_key]" t-as="data">
											<t t-if="(data.form_field_type)=='top_buttons'">
												<t t-foreach="data.form_template_selection_fields" t-as="btn">
													<t t-if="c_order['id']">
														<t t-if="c_order['state'] == 'draft'">
															<t t-if="(btn.value == 'order_confirm') or (btn.value == 'order_cancel') or (btn.value == 'action_confirm')">
																<t t-if="(btn.value == 'order_confirm') or(btn.value == 'action_confirm')">
																	<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
																</t>
																<t t-if="btn.value == 'order_cancel'">
																	<button t-att-id="btn.value" class="header-btn"><t t-esc="btn.name" /></button>
																</t>
															</t>
															<t t-if="btn.value == 'invoice_validate'">
																<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
															</t>
														</t>
														<t t-if="c_order['state'] == 'confirmed'">
															<t t-if="(btn.value == 'button_validate')">
																<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
															</t>
														</t>
														<t t-if="c_order['state'] == 'done'">
															<t t-if="(btn.value == 'order_return')">
																<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
															</t>
															
															<t t-if="(btn.value == 'order_invoice') and (c_order['invoice_ids'] == '')">
																<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
															</t>
														</t>
														<t t-if="c_order['state'] == 'sale'">
															<t t-if="(btn.value == 'order_invoice')">
																<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
															</t>
														</t>
														<t t-if="c_order['state'] == 'open'">
															<t t-if="(btn.value == 'register_payment')">
																<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
															</t>
														</t>
													</t>
													<t t-else="">
														<t t-if="(btn.value == 'order_confirm') or (btn.value == 'order_cancel') or (btn.value == 'action_confirm')">
															<t t-if="(btn.value == 'order_confirm') or (btn.value == 'action_confirm')">
																<button t-att-id="btn.value" class="header-btn btn-active"><t t-esc="btn.name" /></button>
															</t>
															<t t-if="btn.value == 'order_cancel'">
																<button t-att-id="btn.value" class="header-btn"><t t-esc="btn.name" /></button>
															</t>
														</t>
														<t t-else="">
															<button t-att-id="btn.value" style="display:none;" class="header-btn"><t t-esc="btn.name" /></button>
														</t>
													</t>
												</t>
											</t>
										</t>
									</t>
									<t t-if="c_order['id']">
										<span id="order_id" style="display:none;"><t t-esc="c_order['id']" /></span>
									</t>
									<t t-else="" >
										<span id="order_id" style="display:none;">0</span>
									</t>		
			    					<span id="form_temp_id" style="display:none;"><t t-esc="form_temp_id" /></span>
			    					<span id="model_name" style="display:none;"><t t-esc="model_name" /></span>
			    					<span id="vendor_id" style="display:none;"><t t-esc="vendor_id" /></span>
			    					<span id="invoice_ids" style="display:none;"><t t-esc="c_order['invoice_ids']" /></span>
		    					</div>
		    					<div style="float: right;">
		    						<ul class="state-breadcrumb">
		    							<t t-foreach="line_group_key" t-as="line_key" >
			    							<t t-foreach="line_group[line_key]" t-as="data">
												<t t-if="(data.form_field_type)=='statusbar'">
													<t t-set="state_count" t-value="0" />
													<t t-foreach="data.form_template_selection_fields" t-as="state">
														<t t-if="c_order['id']">
															<t t-if="c_order['state'] == state.value" >
																<li><span t-att-id="state.value" class="active"><t t-esc="state.name" /></span></li>
															</t>
															<t t-else="">
																<t t-if="state.value == 'cancel'">
																	<li style="display:none"><span t-att-id="state.value"><t t-esc="state.name" /></span></li>
																</t>
																<t t-else="">
																	<li><span t-att-id="state.value"><t t-esc="state.name" /></span></li>
																</t>
															</t>
															<t t-set="state_count" t-value="state_count + 1" />
														</t>
														<t t-else="" >
															<t t-if="state_count == 0" >
																<li><span t-att-id="state.value" class="active"><t t-esc="state.name" /></span></li>
															</t>
															<t t-else="">
																<t t-if="state.value == 'cancel'">
																	<li style="display:none"><span t-att-id="state.value"><t t-esc="state.name" /></span></li>
																</t>
																<t t-else="">
																	<li><span t-att-id="state.value"><t t-esc="state.name" /></span></li>
																</t>
															</t>
															<t t-set="state_count" t-value="state_count + 1" />
														</t>
													</t>
												</t>
											</t>
		    							</t>
		    						</ul>
		    					</div>
		    				</div>
		    			    <div>
		    			    	<t t-if="c_order['name']">
		    			    		<span style="float: left;margin-left: 29px;font-size: 20px;"><t t-esc="c_order['name']" /></span>
		    			    	</t>
		    					<t t-foreach="line_group_key" t-as="line_key" >
									<t t-foreach="line_group[line_key]" t-as="same_row">
										<t t-if="(same_row.form_field_type)=='view_buttons'">
											<t t-if="(c_order['invoice_count']) and (c_order['invoice_ids'] != '')">
												<t t-foreach="sub_form_template[same_row.sub_template_id]" t-as="temp" >
													<button t-attf-id="#{temp.id}" class="oe-view-btn invoice-action">
														<i class="fa fa-fw o_button_icon fa-money" style="float: left;font-size: 30px;"></i>
														<div class="oe-view-div">
															<span class="oe-view-value"><t t-esc="c_order['invoice_count']" /></span>
															<span class="oe-view-value">Invoice</span>
														</div>
													</button>
												</t>
											</t>
										</t>
									</t>
								</t>
		    				</div> 
		    					<t t-foreach="line_group_key" t-as="line_key" >
			    					<t t-set="first_col" t-value="line_group[line_key][0]" />
			    					<tr>
			    						<!-- First Column -->
			    						<td class="form-td">
			    							<t t-if="(first_col.form_field_type)=='header_label'">
			    								<span t-attf-style="font-size:#{first_col.font_size}px;float: left;margin-left: 20px;"><t t-esc="first_col.form_label" /></span>
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='label'">
			    								<t t-if="(first_col.form_fields)=='date_order' or (first_col.form_fields)=='order_date'">
			    									<span t-attf-id="#{first_col.form_fields}" t-attf-style="font-size:#{first_col.font_size}px;">
			    									<!-- <t t-esc="moment().format('L LT')"/> -->
			    									<t t-esc="c_order[first_col.form_fields] ? c_order[first_col.form_fields] : moment().format('L LT')" />
			    									</span>
			    								</t>
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='input_char'">
			    								<t t-if="(first_col.form_fields)=='guest_name'">
			    									<table style="width:100%;">
			    										<tbody>
			    											<tr>
			    												<td style="width: 50%;">
			    													<!-- <input type="text" list="products" placeholder="Rooms" style="float:right;" id="product_id"
				    													onfocus="this.placeholder=''"/>
				    												<datalist id="products">
										    							<t t-foreach='products' t-as='product'> 
																		    <option t-att-value='product.name' /> 
																	    </t>
																	</datalist> -->
																	<select class="drop-down-select" id="product_id" style="float: right;">
																	 	<t t-foreach='products' t-as='product'> 
																	 		<option t-att-id='product.id'><t t-esc="product.name" /></option>
																	 	</t>
																	</select>
			    												</td>
			    												<td>
			    													<input type="text"  style="float:left;" t-attf-id="#{first_col.form_fields}"
			    													t-attf-placeholder="#{first_col.form_placeholder}" t-attf-value="#{c_order[first_col.form_fields]}"
				    												/>
			    												</td>
			    											</tr>
			    										</tbody>
			    									</table>
			    								</t>
			    								<t t-else="">
				    								<input type="text" t-attf-id="#{first_col.form_fields}" t-attf-placeholder="#{first_col.form_placeholder}" 
				    								t-attf-value="#{c_order[first_col.form_fields]}"/>
			    								</t>
			    								<span style="display:none;"><t t-esc="first_col.form_fields" /></span>
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='many2one'">
					    						<!-- <input class="selection-type" list="cars" onfocus="this.placeholder=''"
					    						t-attf-placeholder="#{first_col.form_placeholder}" t-attf-id="#{first_col.form_fields}"/>
					    						<img  t-att-src='widget.car_icon_url(1)' width="35" height="32" />
					    						<datalist id="cars">
					    							<t t-foreach='widget.pos.car_types' t-as='car'> 
													    <option t-att-value='car.name' /> 
												    </t>
												 </datalist> -->
												 <select onchange="this.className=this.options[this.selectedIndex].className" t-attf-class="#{(first_col.form_fields) == 'car_type_id' ? 'selection-type placeholder' : 'drop-down-select placeholder' }" t-attf-id="#{first_col.form_fields}">
												 	<option disabled="disabled" selected="selected" class="placeholder"><t t-esc="first_col.form_placeholder" /></option>
												 	<t t-foreach='first_col.form_template_model_fields' t-as='user'> 
														<t t-if="c_order[first_col.form_fields] == user.display_name">
													 		<option t-att-id='user.id' t-att-selected="true" t-attf-class="#{(first_col.form_fields) == 'car_type_id' ? 'selection-type' : 'drop-down-select' }"><t t-esc="user.display_name" /></option>
													 	</t>
													 	<t t-else="">
													 		<option t-att-id='user.id' t-attf-class="#{(first_col.form_fields) == 'car_type_id' ? 'selection-type' : 'drop-down-select' }"><t t-esc="user.display_name" /></option>
													 	</t>
													</t>
												</select>
												<t t-if="(first_col.form_fields) == 'car_type_id'" >
													<img  id="car-image" t-att-src='widget.car_icon_url(first_col.form_template_model_fields[0].id)' width="35" height="32" />
												</t>
												<!-- <t t-if="(first_col.form_fields) == 'laundry_person'" >
													<select class="selection-type" t-attf-id="#{first_col.form_fields}">
													 	<t t-foreach='first_col.form_template_model_fields' t-as='user'> 
													 		<t t-if="c_order[first_col.form_fields] == user.name">
													 			<option t-att-id='user.id' t-att-selected="true"><t t-esc="user.name" /></option>
													 		</t>
													 		<t t-else="">
													 			<option t-att-id='user.id'><t t-esc="user.name" /></option>
													 		</t>
													 	</t>
													 </select>
												</t>
												<t t-else="">
													 <select class="selection-type" t-attf-id="#{first_col.form_fields}">
													 	<t t-foreach='first_col.form_template_model_fields' t-as='car'> 
													 		<t t-if="c_order[first_col.form_fields] == car.name">
													 			<option t-att-id='car.id' t-att-selected="true"><t t-esc="car.name" /></option>
													 		</t>
													 		<t t-else="">
													 			<option t-att-id='car.id'><t t-esc="car.name" /></option>
													 		</t>
													 	</t>
													 </select>
													 <img  id="car-image" t-att-src='widget.car_icon_url(first_col.form_template_model_fields[0].id)' width="35" height="32" />
					    						</t> -->
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='date'">
			    								<div class="span5 col-md-5 date-picker" id="sandbox-container">
				    								<div class="input-daterange input-group" id="datepicker">
					    								<i class="fa fa-calendar icon"></i>
					    								<input type="text" class="input-sm form-control datepicker-input"  t-attf-placeholder="#{first_col.form_placeholder}" 
					    								t-attf-id="#{first_col.form_fields}" t-attf-value="#{c_order[first_col.form_fields]}"
					    								/>
				    								</div>
			    								</div>
			    								<span style="display:none;"><t t-esc="first_col.form_fields" /></span>
			    							</t>
			    						</td>
			    						
			    						<!-- Same Row -->
			    						<t t-foreach="line_group[line_key]" t-as="same_row">
			    							<t t-if="same_row.sameline">
			    								<td class="form-td">
					    							<t t-if="(same_row.form_field_type)=='header_label'">
