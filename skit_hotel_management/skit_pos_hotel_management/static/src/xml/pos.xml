<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

	<t t-name="HotelManagementWidget">
        <div class='product-list-container'>
            <div class="product-list-scroller touch-scrollable">
                <div class="hotel">
                	<span>Hotel Management</span>
                </div>
            </div>
            <span class="placeholder-ScrollbarWidget" />
        </div>
    </t>

	<t t-name="HotelManagementScreenWidget">
       <div class="hotel-screen screen">
       		<span>dfhjdskfhjfdk</span>
       		<input type="date" name="my_date" />
        	<span class="vendor-dash-button">Vendor Dashboard</span>
        </div>
    </t>
    
    <t t-name="VendorListContent">
    	<div style="display: inline-block;width: 100%;height: 87%;overflow-y: auto;">
    		<h1 t-attf-style="text-align: left;border-bottom: 0.1px solid gray;color:#{color}"><t t-esc="form_name" /></h1>
    		<!-- Kanban View -->
    		<t t-if="form_view == 'kanban'">
		    	<t t-foreach="result_datas" t-as="vendor" >
					<div class="vendor-tag" >
						<div class="vendor-list">
							<t t-foreach="line_group_key" t-as="line_key" >
								<t t-set="first_col" t-value="line_group[line_key][0]" />
							 	<t t-if="(first_col.form_field_type)=='image'">
									<div class="vendor-image">
										<img class="header-text" t-att-src='widget.vendor_image_url(vendor.id)' alt="Vendor Iamge"/>
									</div>
								</t>
							</t>
							<div class="vendor-addr">
								<t t-set="count" t-value="1" />
								<t t-foreach="line_group_key" t-as="line_key" >
									<t t-if="count != 1" >
										<t t-foreach="line_group[line_key]" t-as="same_row">
											<t t-if="(same_row.form_field_type)=='label'">
												<ul>
										        	<li t-if="vendor[same_row.form_fields]" t-attf-style="font-size:#{same_row.font_size}px; font-style:#{same_row.font_style}; font-family:#{same_row.font_family};"><t t-esc="vendor[same_row.form_fields]" /></li>
												</ul>
											</t>
										</t> 
									</t>
									<t t-set="count" t-value="count + 1" />
								</t>
							</div>
						</div>
						<div class="vendor-process">
							<ul>
								<!-- <li class="order_icon" style="background-color: #00BFFF;"><img  src="/skit_pos_hotel_management/static/src/img/order_icon.png" width="24" height="21" /></li>
								<li class="delivery_icon" style="background-color: #483D8B;"><img  src="/skit_pos_hotel_management/static/src/img/delivery_icon.png" width="24" height="21" /></li>
								<li class="payment_icon" style="background-color: #819FF7;"><img  src="/skit_pos_hotel_management/static/src/img/payment_icon.png" width="24" height="21" /></li> -->
								<t t-foreach="sub_form_template" t-as="temp" >
									<li t-attf-id="#{temp.id}" class="vendor-form-icon" t-attf-style="background-color: #{temp.color}">
										<img  t-att-src='widget.sub_template_icon_url(temp.id)' width="24" height="21" />
									</li>
								</t>
							</ul>
						</div>
					</div>
		    	</t>
	    	</t>
	    	
	    	<!-- List View -->
	    	<t t-if="form_view == 'list'">
	    		<table id="vendor_order_list" class="table table-striped table-hover table-condensed table-responsive item-table">
	    			<thead>
	    				<t t-set="head_count" t-value="1" />
	    				<t t-foreach="line_group_key" t-as="line_key" >
	    					<t t-if="head_count == 1" >
			    				<tr style="background-color: #875A7B;color: white;">
						    		<t t-foreach="line_group[line_key]" t-as="label">
						    			<th><t t-esc="label.form_label" /></th>
						    		</t>
					    		</tr>
					    	<t t-set="head_count" t-value="head_count + 1" />
				    		</t>
			    		</t>
	    			</thead>
	    			<tbody>
	    				<t t-set="count" t-value="1" />
	    				<t t-foreach="result_datas" t-as="order" >
		    				<t t-foreach="line_group_key" t-as="line_key" >
		    					<t t-set="first_col" t-value="line_group[line_key][0]" />
		    					<!-- <t t-if="count == 1" >
			    					<tr style="background-color: #875A7B;color: white;">
			    						<t t-foreach="line_group[line_key]" t-as="label">
			    							<th><t t-esc="label.form_label" /></th>
			    						</t>
			    					</tr>
		    					</t> -->
		    					<tr>
		    						<td>
		    							<t t-if="(first_col.form_field_type)=='label'">
		    								<span><t t-esc="order[first_col.form_fields]" /></span>
		    							</t>
		    						</td>
		    						<t t-foreach="line_group[line_key]" t-as="same_row">
		    							<t t-if="same_row.sameline">
		    								<td>
				    							<t t-if="(same_row.form_field_type)=='label'">
				    								<span><t t-esc="order[same_row.form_fields]" /></span>
				    							</t>
				    						</td>
		    							</t>
		    						</t>
		    					</tr>
		    				</t>
		    			<t t-set="count" t-value="count + 1" />
	    				</t>
	    			</tbody>
	    		</table>
	    	</t>
	    	<!-- Form View -->
	    	<t t-if="form_view == 'form'">
	    		<div style="display: inline-block;width: 100%;">
	    			<button class="back-btn" style="float: left;background: #147FD0;color: white;">Back</button>
	    		</div>
	    		<div>
	    		<t t-if="line_group_key">
	    			<div style="width:98%;border: 1px solid gray;">
		    			<table class="order-form-detail">
		    				<tbody>
		    				<div style="width:100%;border-bottom: 1px solid gray;background-color: #F8F8F8;display: inline-block;">
		    					<button style="background: white;padding: 10px;float: left;margin-left: 20px;font-size: 14px;">Confirm</button>
		    					<button style="background: white;padding: 10px;float: left;margin-left: 20px;font-size: 14px;">cancel</button>
		    				</div>
		    				<span>TEST</span>
		    					<t t-foreach="line_group_key" t-as="line_key" >
			    					<t t-set="first_col" t-value="line_group[line_key][0]" />
			    					<tr>
			    						<!-- First Column -->
			    						<td style="border: none;">
			    							<t t-if="(first_col.form_field_type)=='header_label'">
			    								<span t-attf-style="font-size:#{first_col.font_size}px;float: left;margin-left: 20px;"><t t-esc="first_col.form_label" /></span>
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='label'">
			    								<t t-if="(first_col.form_fields)=='date_order'">
			    									<span t-attf-style="font-size:#{first_col.font_size}px;"><t t-esc="moment().format('L LT')"/></span>
			    								</t>
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='input_char'">
			    								<input type="text" t-attf-value="#{first_col.form_field_id}" />
			    								<span style="display:none;"><t t-esc="first_col.form_fields" /></span>
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='selection'">
			    								<select class="selection-type">
						                            <t t-foreach='widget.pos.car_types' t-as='car'>                            	
						                                <option t-att-value='car.id'> 
						                                    <t t-esc='car.name'/>
						                                </option>
						                            </t>
					    						</select>
					    						<img  t-att-src='widget.car_icon_url(1)' width="35" height="32" />
			    							</t>
			    							<t t-if="(first_col.form_field_type)=='date'">
			    								<input type="text" t-attf-value="#{first_col.form_field_id}" />
			    								<span style="display:none;"><t t-esc="first_col.form_fields" /></span>
			    							</t>
			    						</td>
			    						<!-- Same Row -->
			    						<t t-foreach="line_group[line_key]" t-as="same_row">
			    							<t t-if="same_row.sameline">
			    								<td style="border: none;">
					    							<t t-if="(same_row.form_field_type)=='header_label'">
					    								<span t-attf-style="font-size:#{same_row.font_size}px;float: left;margin-left: 20px;"><t t-esc="same_row.form_label" /></span>
					    							</t>
					    							<t t-if="(same_row.form_field_type)=='label'">
					    								<t t-if="(same_row.form_fields)=='date_order'">
					    									<span t-attf-style="font-size:#{same_row.font_size}px;"><t t-esc="moment().format('L LT')"/></span>
					    								</t>
					    							</t>
					    							<t t-if="(same_row.form_field_type)=='input_char'">
					    								<input type="text" t-attf-value="#{first_col.form_field_id}" />
					    								<span style="display:none;"><t t-esc="same_row.form_fields" /></span>
					    							</t>
					    							<t t-if="(same_row.form_field_type)=='selection'">
					    								<select class="selection-type">
								                            <t t-foreach='widget.pos.car_types' t-as='car'>                            	
								                                <option t-att-value='car.id'> 
								                                    <t t-esc='car.name'/>
								                                </option>
								                            </t>
							    						</select>
					    								<img  t-att-src='widget.car_icon_url(1)' width="35" height="32" />
					    							</t>
					    							<t t-if="(same_row.form_field_type)=='date'">
					    								<input type="text" t-attf-value="#{first_col.form_field_id}" />
					    								<span style="display:none;"><t t-esc="same_row.form_fields" /></span>
					    							</t>
					    						</td>
			    							</t>
			    						</t>
			    					</tr>
			    				</t>
		    				</tbody>
		    			</table>
	    			</div>
	    		</t>
	    		</div>
	    	</t>
    	</div>
    </t>
    
    <t t-name="VendorDashboard">
	    <section style="height:88vh;">
	    	<t t-foreach="dashboards" t-as="dashboard">
				<div class="item" t-attf-style="background-color:#{dashboard.color};">
					<div>
		                <img class="dash-icon" t-att-src='widget.dashboard_icon_url(dashboard.id)' alt="Dashboard logo"/>
		            </div>
					<h1 class="horizontal-text"><t t-esc="dashboard.vendor_category_id[1]" /></h1>	
					<t t-if="widget.get_vendors(dashboard.vendor_category_id[0])" >
						<h1 class="vendor-count"><t t-esc="(widget.get_vendors(dashboard.vendor_category_id[0])).length" /></h1>
					</t>
				</div>			
				<div class="info" t-attf-style="background-color:#{dashboard.color};">
					<div class="info1" t-attf-style="display:inline-block;float: left;width:4vw;height:0vh;">
						<t t-if="widget.get_dashboard_line(dashboard.id)" >
							<table style="width:100%;margin-top: 20px;display: inline-block;">
								<tbody style="display: inline-block;width: 100%;">
									<t t-foreach="widget.get_dashboard_line(dashboard.id)" t-as="line" >
										<tr style="display: inline-block;width: 100%;">
											<td style="border: none;padding: 0px;border-bottom: 1px solid darkgray;display: inline-block;width: 100%;">
												<div class="test">
									                <img t-attf-id="#{line.id}" t-attf-menu="#{line.dashboard_menu[1]}" t-attf-dashid="#{dashboard.id}" t-attf-color="#{dashboard.color}"
									                t-attf-categid="#{dashboard.vendor_category_id[0]}" 
									                class="dash-icon dashboard-element" t-att-src='widget.dashboard_line_icon_url(line.id)' alt="Dashboard logo"/>
									            </div>
											</td>
										</tr>
									</t>
								</tbody>
							</table>
						</t>
					</div>
					<div class="info2" t-attf-style="display:inline-block;float: right;width:94.5%;background-color: white;color: black;height:86vh;border-top: 6px solid #{dashboard.color};border-bottom: 6px solid #{dashboard.color};border-right: 6px solid #{dashboard.color};">
						<div style="display: inline-block;width: 100%;">
							<img class="header-text dash-icon1" t-attf-style="filter: opacity(0.5) drop-shadow(#{dashboard.color} 0px 0px 0px);" t-att-src='widget.dashboard_icon_url(dashboard.id)' alt="Dashboard logo"/>
							<h2 class="header-text" t-attf-style="color:#{dashboard.color};"><t t-esc="dashboard.vendor_category_id[1]" /></h2>
						</div>
						<div style="display: inline-block;width: 94%;height: 87%;" class="vendor-contents">
                		</div>
					</div>
				</div>	
			</t>
		</section>
