<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="car_service" model="product.product">
        <field name="name">Car Rental</field>
        <field name="type">service</field>
    </record>
    <record id="health_service" model="product.product">
        <field name="name">Doctor Service</field>
        <field name="type">service</field>
    </record>
    
    <!-- Sequence number for ACI -->
	<record id="seq_number_for_ict" model="ir.sequence">
		<field name="name">Agent Commission Invoice</field>
		<field name="code">hm.agent.commission.invoice</field>
		<field name="prefix">ACI</field>
		<field name="padding">6</field>
		<field name="company_id" eval="False" />
	</record>
</odoo>