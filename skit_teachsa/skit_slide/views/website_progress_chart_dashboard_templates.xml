<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="progess_chart_dashboard_assets_frontend"
		inherit_id="website.assets_frontend" name="Progess Chart Dashboard Front-end assets">
		<xpath expr="//link[last()]" position="after">
			<link rel="stylesheet"
				href="/skit_slide/static/src/css/progress_chart_dashboard.css" />
		</xpath>
		<xpath expr="//script[last()]" position="after">
			<script type="text/javascript"
				src="/skit_slide/static/src/js/progress_chart_dashboard.js"></script>
		</xpath>
	</template>
	
	<template id="progress_chart_dashboard">
		<div class="chart_dashboard_header_div">
			<div id="marks_chart" class="chart_dashboard_inner_div floatleft">
			</div>

			<div id="exam_chart"
				class="chart_dashboard_inner_div chart_dashboard_div floatleft">
			</div>

			<div id="duration_chart" class="chart_dashboard_inner_div floatleft">
			</div>
		</div>
	</template>
</odoo>